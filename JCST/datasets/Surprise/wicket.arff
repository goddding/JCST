id populateItem(ListItem item) { IChoiceRenderer renderer = new IChoiceRenderer() { public Object getDisplayValue(Object object) { return object; } public String getIdValue(Object object  int index) { return (String) object; } }; final List selected = new ArrayList(); IModel model = new IModel() { public Object getObject() { return selected; } public void setObject(Object object) { selected.clear(); selected.addAll((List) object); } public void detach() { } }; IModel choicesModel = new AbstractReadOnlyModel() { @Override public Object getObject() { final List&lt;String&gt; choices = new ArrayList&lt;String&gt;(); choices.add(\'1\'); choices.add(\'2\'); return choices; } }; item.add(new Palette(\'palette\'  model  choicesModel  renderer  10  false)); } }); add(new FeedbackPanel(\'feedback\'  new ContainerFeedbackMessageFilter(this))); }}   ',0
